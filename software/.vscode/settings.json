{
  "C_Cpp.clang_format_style": "Visual Studio",
  "editor.formatOnSave": false,
  "[cpp]": {
    "editor.quickSuggestions": true
  },
  "[c]": {
    "editor.quickSuggestions": true
  },
  "C_Cpp.intelliSenseEngineFallback": "Enabled",
  "files.associations": {
    "*.inc": "c",
    "*.tcc": "cpp",
    "complex": "cpp",
    "optional": "cpp",
    "random": "cpp",
    "future": "cpp",
    "istream": "cpp",
    "limits": "cpp",
    "streambuf": "cpp",
    "*.ipp": "cpp",
    "iomanip": "cpp",
    "stdexcept": "cpp",
    "typeinfo": "cpp",
    "cmath": "cpp",
    "iosfwd": "cpp",
    "cstdio": "cpp",
    "fstream": "cpp",
    "deque": "c",
    "list": "c",
    "string": "c",
    "unordered_map": "c",
    "vector": "c",
    "system_error": "c",
    "array": "c",
    "string_view": "c",
    "initializer_list": "c",
    "utility": "c",
    "json.h": "c",
    "esp_ota_ops.h": "c",
    "esp_http_server.h": "c",
    "esp_wifi.h": "c",
    "esp_wifi_types.h": "c",
    "esp_event.h": "c",
    "configuration.h": "c",
    "esp_spiffs.h": "c",
    "semphr.h": "c",
    "task.h": "c",
    "cstring": "cpp",
    "chrono": "cpp",
    "ctime": "cpp",
    "numeric": "cpp",
    "ratio": "cpp",
    "tuple": "cpp",
    "type_traits": "cpp"
  },
  "cmake.configureOnOpen": false,
  "cSpell.words": [
    "IRAM",
    "LOGD",
    "LOGI",
    "LOGV",
    "LOGW",
    "ledc",
    "mdio",
    "qclass",
    "rddata",
    "rmii",
    "ssid"
  ]
}
